<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vboxTemplate" xmlns="http://javafx.com/javafx/8.0.162-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.heitx.maserow.ui.item.template.ItemTemplateController">
   <children>
      <HBox alignment="CENTER_LEFT" prefWidth="200.0" spacing="5.0">
         <children>
            <Button fx:id="btnExecute" defaultButton="true" mnemonicParsing="false" text="Execute" />
            <Button fx:id="btnScreenshot" mnemonicParsing="false" text="Save as Image" />
            <MenuButton fx:id="mbSQL" mnemonicParsing="false" text="Save as SQL">
              <items>
                <MenuItem fx:id="miInsert" mnemonicParsing="false" text="INSERT" />
                <MenuItem fx:id="miUpdate" mnemonicParsing="false" text="UPDATE" />
                  <MenuItem fx:id="miDelete" mnemonicParsing="false" text="DELETE" />
              </items>
            </MenuButton>
            <CheckBox fx:id="cbPreview" mnemonicParsing="false" text="Preview" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
       <fx:include fx:id="build" source="build/itembuild.fxml" />
       <fx:include fx:id="preview" source="preview/itempreview.fxml" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
