<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="vboxTemplate" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" spacing="1.0" xmlns="http://javafx.com/javafx/8.0.162-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.heitx.maserow.ui.item.template.ItemTemplateController">
   <children>
      <HBox alignment="CENTER_LEFT" spacing="5.0">
         <children>
            <Button fx:id="btnExecute" mnemonicParsing="false" text="Execute" />
            <Button fx:id="btnScreenshot" mnemonicParsing="false" text="Save as Image" />
            <MenuButton fx:id="mbSQL" mnemonicParsing="false" text="Save as SQL">
              <items>
                <MenuItem fx:id="miInsert" mnemonicParsing="false" text="INSERT" />
                <MenuItem fx:id="miUpdate" mnemonicParsing="false" text="UPDATE" />
                  <MenuItem fx:id="miDelete" mnemonicParsing="false" text="DELETE" />
              </items>
            </MenuButton>
            <CheckBox fx:id="cbPreview" mnemonicParsing="false" text="Preview" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
       <VBox prefHeight="200.0" prefWidth="100.0">
           <children>
               <fx:include fx:id="build" source="build/itembuild.fxml" />
               <fx:include fx:id="preview" source="preview/itempreview.fxml" />
           </children>
       </VBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
