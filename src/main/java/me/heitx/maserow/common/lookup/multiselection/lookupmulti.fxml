<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox fx:id="vbRoot" maxHeight="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefWidth="400.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.202-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.heitx.maserow.common.lookup.multiselection.LookupMultiController">
   <children>
      <HBox alignment="CENTER">
         <children>
            <Label fx:id="labelTitle" text="Title">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <Pane HBox.hgrow="ALWAYS" />
            <Button fx:id="btnEscape" mnemonicParsing="false" text="X" />
         </children>
      </HBox>
      <HBox spacing="10.0">
         <children>
            <TextField fx:id="tfFilter" maxWidth="1.7976931348623157E308" promptText="Filter by name" HBox.hgrow="ALWAYS" />
            <Button fx:id="btnFilter" mnemonicParsing="false" text="Filter" />
         </children>
      </HBox>
      <ScrollPane fitToHeight="true" fitToWidth="true" VBox.vgrow="ALWAYS">
         <content>
            <TableView fx:id="tvTable" editable="true">
              <columns>
                <TableColumn fx:id="tcEntry" editable="false" maxWidth="75.0" minWidth="75.0" prefWidth="75.0" text="entry" />
                <TableColumn fx:id="tcValue" editable="false" maxWidth="90.0" minWidth="90.0" text="value" />
                  <TableColumn fx:id="tcName" editable="false" prefWidth="75.0" text="name" />
                  <TableColumn fx:id="tcSelected" maxWidth="30.0" minWidth="40.0" prefWidth="40.0" sortable="false" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </content>
      </ScrollPane>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <CheckBox fx:id="cbEntryColumn" mnemonicParsing="false" text="Show Entry" />
            <CheckBox fx:id="cbValueColumn" mnemonicParsing="false" text="Show Value" />
            <Pane HBox.hgrow="ALWAYS" />
            <Button fx:id="btnSelect" mnemonicParsing="false" text="Select" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
