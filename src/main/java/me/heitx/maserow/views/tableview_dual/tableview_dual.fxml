<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="600.0" prefWidth="800.0" spacing="5.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.heitx.maserow.views.tableview_dual.TableviewDualController">
   <VBox alignment="TOP_CENTER" HBox.hgrow="SOMETIMES">
      <children>
         <Label fx:id="labelFirst" text="...">
            <font>
               <Font name="System Bold" size="18.0" />
            </font>
         </Label>
        <TableView fx:id="tvFirst" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
          <columns>
            <TableColumn fx:id="tcFirstId" editable="false" maxWidth="75.0" minWidth="75.0" prefWidth="75.0" text="id" />
            <TableColumn fx:id="tcFirstBitmask" editable="false" maxWidth="100.0" minWidth="75.0" prefWidth="100.0" text="bitmask" />
            <TableColumn fx:id="tcFirstName" editable="false" text="name" />
            <TableColumn fx:id="tcFirstDescription" editable="false" text="description" />
          </columns>
          <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
          </columnResizePolicy>
        </TableView>
      </children>
   </VBox>
   <VBox alignment="TOP_CENTER" HBox.hgrow="SOMETIMES">
      <children>
         <Label fx:id="labelSecond" text="...">
            <font>
               <Font name="System Bold" size="18.0" />
            </font>
         </Label>
        <TableView fx:id="tvSecond" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
          <columns>
            <TableColumn fx:id="tcSecondId" editable="false" maxWidth="75.0" minWidth="75.0" prefWidth="75.0" text="id" />
            <TableColumn fx:id="tcSecondBitmask" editable="false" maxWidth="100.0" minWidth="75.0" prefWidth="100.0" text="bitmask" />
            <TableColumn fx:id="tcSecondName" editable="false" text="name" />
            <TableColumn fx:id="tcSecondDescription" editable="false" text="description" />
          </columns>
          <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
          </columnResizePolicy>
        </TableView>
      </children>
   </VBox>
</HBox>
