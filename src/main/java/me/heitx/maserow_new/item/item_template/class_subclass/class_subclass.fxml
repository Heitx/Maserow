<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.heitx.maserow_new.item.item_template.class_subclass.ClassSubclassController">
   <VBox alignment="TOP_CENTER" HBox.hgrow="SOMETIMES">
      <children>
         <Label text="Class">
            <font>
               <Font name="System Bold" size="18.0" />
            </font>
         </Label>
        <TableView fx:id="tvClass" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
          <columns>
            <TableColumn fx:id="tcClassId" editable="false" maxWidth="75.0" minWidth="75.0" prefWidth="75.0" text="id" />
            <TableColumn fx:id="tcClassName" editable="false" prefWidth="75.0" text="name" />
          </columns>
          <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
          </columnResizePolicy>
        </TableView>
      </children>
   </VBox>
   <VBox alignment="TOP_CENTER" HBox.hgrow="SOMETIMES">
      <children>
         <Label text="Subclass">
            <font>
               <Font name="System Bold" size="18.0" />
            </font>
         </Label>
        <TableView fx:id="tvSubclass" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
          <columns>
            <TableColumn fx:id="tcSubclassId" editable="false" maxWidth="90.0" minWidth="90.0" text="id" />
            <TableColumn fx:id="tcSubclassName" editable="false" prefWidth="75.0" text="name" />
          </columns>
          <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
          </columnResizePolicy>
        </TableView>
      </children>
   </VBox>
</HBox>
